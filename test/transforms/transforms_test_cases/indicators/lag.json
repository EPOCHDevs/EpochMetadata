{
  "tests": [
    {
      "title": "Lag Period 1",
      "input": {
        "data": [
          10.0,
          20.0,
          30.0,
          40.0,
          50.0
        ]
      },
      "expect": {
        "type": "dataframe",
        "columns": {
          "lag#result": [
            null,
            10.0,
            20.0,
            30.0,
            40.0
          ]
        }
      },
      "options": {
        "transform_name": "lag",
        "output_id": "lag",
        "inputs": "SLOT: data",
        "period": 1,
        "timeframe": "1D"
      }
    },
    {
      "title": "Lag Period 2",
      "input": {
        "data": [
          100.0,
          200.0,
