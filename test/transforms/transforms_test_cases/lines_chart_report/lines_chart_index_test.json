{
  "tests": [
    {
      "title": "Lines Chart Report with Index Column",
      "input": {
        "timestamp": [
          "2024-01-01T09:00:00",
          "2024-01-01T10:00:00",
          "2024-01-01T11:00:00",
          "2024-01-01T12:00:00",
          "2024-01-01T13:00:00"
        ],
        "portfolio#returns": [
          0.05,
          -0.02,
          0.03,
          0.08,
          -0.01
        ]
      },
      "expect": {
        "type": "tearsheet",
        "charts": [
          {
            "title": "Returns Over Time",
            "category": "Charts",
            "type": "WidgetLinesChart",
            "x_axis": {
              "data": [
                0.0,
                1.0,
                2.0,
                3.0,
                4.0
              ],
              "type": "TypeInteger"
            },
            "lines": [
              {
                "name": "returns",
                "data": [
                  0.05,
                  -0.02,
                  0.03,
                  0.08,
                  -0.01
                ],
                "type": "TypeDecimal"
              }
            ]
          }
        ]
      },
      "options": {
        "transform_name": "lines_chart_report",
        "title": "Returns Over Time",
        "use_index": true,
        "index_column": "time_index",
        "y_value_column": "portfolio_returns",
        "x_axis_title": "Time Index",
        "y_axis_title": "Returns (%)",
        "output_id": "test_index_lines_chart",
        "inputs": "{SLOT: [portfolio#returns]}",
        "timeframe": "1D"
      },
      "timestamp_columns": [
        "timestamp"
      ],
      "index_column": "timestamp"
    }
  ]
}